@model IEnumerable<Category> 

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<h2>Index Category</h2>
<table class= "table table-bordered table-striped" style="width:100%">
    <thead>
    <tr>
        <td>
            Category Name
        </td>
        <th>
            Display Order
        </th>
    </tr>
    </thead>
    <tbody>
    @foreach (var obj in Model)
    {
        <tr>
            <td width="50%">
                @obj.Name
            </td>
            <td width="30%">
                @obj.Description
            </td>
        </tr>
    }
    </tbody>
</table>
